{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Subscribe or unsubscribe to our email list",
          "examplePhrases": [            
            "Alexa start email subscriber",
            "Alexa launch email subscriber",
            "Alexa, ask email subscriber to subscribe"
          ],
          "name": "email-subscriber",
          "description": "Subscribe or unsubscribe to our email list",
          "smallIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-sm.png",
          "largeIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-lg.png"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "No special instructions.",
      "category": "EDUCATION_AND_REFERENCE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        },
        "interfaces": [
          {
            "type": "ALEXA_PRESENTATION_APL"
          }
        ]
      }
    },
    "manifestVersion": "1.0",
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "http://dabblelab.com/terms",
          "privacyPolicyUrl": "http://dabblelab.com/privacy"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    },
    "permissions": [
      {
        "name": "alexa::profile:email:read"
      },
      {
        "name": "alexa::profile:name:read"
      }
    ]
  }
}
